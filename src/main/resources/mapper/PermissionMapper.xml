<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.clay.system.mapper.PermissionMapper">


    <select id="queryAll" resultType="com.clay.system.model.enity.Permission">
        SELECT * FROM t_permission
    </select>
    <select id="queryById" resultType="com.clay.system.model.enity.Permission">
        SELECT * FROM t_permission WHERE id = #{id}
    </select>
</mapper>